<Defs>

  <!-- Designator Dropdown Group Def for Big batteries -->
  <DesignatorDropdownGroupDef>
    <defName>Big_Batteries</defName>
  </DesignatorDropdownGroupDef>

  <!-- Big Lead Battery -->
  <ThingDef ParentName="BaME_BatteryBase">
    <defName>BaME_Big_Lead_Battery</defName>
    <label>big lead battery</label>
    <description>A bigger version of the lead battery. Stores larges amounts of electricity for later.</description>
    <designatorDropdown>Big_Batteries</designatorDropdown>
    <graphicData>
      <texPath>Things/Building/Power/Big_Batteries/BaME_Big_Lead_Battery</texPath>
      <drawSize>(3,3)</drawSize>
    </graphicData>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>5000</WorkToBuild>
      <Mass>180</Mass>
      <Flammability>0.25</Flammability>
      <Beauty>-15</Beauty>
    </statBases>
    <descriptionHyperlinks>
      <ThingDef>BaME_Big_Copper_Battery</ThingDef>
      <ThingDef>BaME_Big_Steel_Battery</ThingDef>
      <ThingDef>BaME_Big_Waterproof_Battery</ThingDef>
      <ThingDef>BaME_Big_Nickel_Battery</ThingDef>
      <ThingDef>BaME_Big_Uranium_Battery</ThingDef>
      <ThingDef>BaME_Big_Lithium_Battery</ThingDef>
      <ThingDef>BaME_Big_Plasteel_Battery</ThingDef>
    </descriptionHyperlinks>
    <size>(2,3)</size>
    <building>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
    </building>
    <costList>
      <BaME_Lead>195</BaME_Lead>
      <Steel>60</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <comps>
      <li Class="CompProperties_Battery">
        <storedEnergyMax>2500</storedEnergyMax>
        <efficiency>0.5</efficiency>
        <shortCircuitInRain>true</shortCircuitInRain>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <uiOrder>2021</uiOrder>
    <researchPrerequisites>
      <li>BaME_Lead_Batteries</li>
      <li>Big_Batteries</li>
    </researchPrerequisites>
    <staticSunShadowHeight>0.3</staticSunShadowHeight>
  </ThingDef>

