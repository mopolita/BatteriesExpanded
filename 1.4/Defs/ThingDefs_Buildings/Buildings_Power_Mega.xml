  <!-- Designator Dropdown Group Def for mega batteries -->
  <DesignatorDropdownGroupDef>
    <defName>Mega_Batteries</defName>
  </DesignatorDropdownGroupDef>

  <!-- Mega Lead Battery -->
  <ThingDef ParentName="BaME_BatteryBase">
    <defName>BaME_Mega_Lead_Battery</defName>
    <label>giant lead battery</label>
    <description>Stores larges amounts of electricity for later.</description>
    <designatorDropdown>Mega_Batteries</designatorDropdown>
    <graphicData>
      <texPath>Things/Building/Power/BaME_Mega_Lead_Battery</texPath>
      <drawSize>(4,5)</drawSize>
    </graphicData>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>5000</WorkToBuild>
      <Mass>180</Mass>
      <Flammability>0.25</Flammability>
      <Beauty>-15</Beauty>
    </statBases>
    <descriptionHyperlinks>
      <ThingDef>BaME_Mega_Copper_Battery</ThingDef>
      <ThingDef>BaME_Mega_Steel_Battery</ThingDef>
      <ThingDef>BaME_Mega_Waterproof_Battery</ThingDef>
      <ThingDef>BaME_Mega_Nickel_Battery</ThingDef>
      <ThingDef>BaME_Mega_Uranium_Battery</ThingDef>
      <ThingDef>BaME_Mega_Lithium_Battery</ThingDef>
      <ThingDef>BaME_Mega_Plasteel_Battery</ThingDef>
    </descriptionHyperlinks>
    <size>(2,3)</size>
    <building>
      <destroySound>BuildingDestroyed_Metal_Large</destroySound>
    </building>
    <costList>
      <BaME_Lead>195</BaME_Lead>
      <Steel>60</Steel>
      <ComponentIndustrial>3</ComponentIndustrial>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <comps>
      <li Class="CompProperties_Battery">
        <storedEnergyMax>2500</storedEnergyMax>
        <efficiency>0.5</efficiency>
        <shortCircuitInRain>true</shortCircuitInRain>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <uiOrder>2021</uiOrder>
    <researchPrerequisites>
      <li>BaME_Lead_Batteries</li>
      <li>Mega_Batteries</li>
    </researchPrerequisites>
  </ThingDef>

